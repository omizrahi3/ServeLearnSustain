  <body>


    <!-- Admin Functionalities -->

      <!-- Pending Data Points -->
      <div class="container">
        <div class="card mt-5">
          <div class="card-block">
            <h2 class="display-5 text-center">Pending City Official Accounts</h2>

            <!-- table of data points -->
            <div class="table-responsive mb-4">
              <table class="table">
                <thead>
                  <tr>
                    <th>Select</th>
                    <th>Username</th>
                    <th>Email</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Title</th>
                  </tr>
                </thead>
                <tbody>
                  <th scope="row"><input type="checkbox" class="form-check-input ml-3"></th>
                  <td>dummy</td>
                  <td>dummy</td>
                  <td>dummy</td>
                  <td>dummy</td>
                  <td>dummy</td>
                </tbody>
              </table>
            </div>

            <div class="d-flex justify-content-start">
              <a href="admin-dashboard.ejs" class="p-2 btn btn-outline-success">Back</a>
              <button type="button" class="ml-auto p-2 btn btn-outline-primary">Reject</button>
              <button type="button" class="ml-auto p-2 btn btn-outline-primary">Accept</button>
            </div>
          </div>
        </div>
      </div>



    <!-- jQuery first, then Tether, then Bootstrap JS. -->
